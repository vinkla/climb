{
    "name": "vinkla/climb",
    "description": "A Composer version manager tool",
    "license": "MIT",
    "keywords": ["composer", "version", "cli", "command"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "symfony/console": "^2.7.3",
        "league/climate": "^3.2.0",
        "knplabs/packagist-api": "^1.3.0",
        "composer/semver": "^1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.0.0",
        "mockery/mockery": "^0.9.4"
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Climb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Climb\\": "tests/"
        }
    },
    "bin": [
        "bin/climb"
    ],
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
