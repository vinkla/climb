{
    "name": "vinkla/climb",
    "description": "A Composer version manager tool",
    "license": "MIT",
    "keywords": ["composer", "version", "cli", "command"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        }
    ],
    "require": {
        "symfony/console": "^2.7.3",
        "league/climate": "^3.2.0",
        "knplabs/packagist-api": "^1.2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.8.6"
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Climb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Climb\\Tests\\": "tests/"
        }
    },
    "bin": [
        "climb"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
