{
    "name": "vinkla/climb",
    "description": "A Composer version manager tool",
    "license": "MIT",
    "keywords": ["composer", "version", "cli", "command"],
    "authors": [
        {
            "name": "Vincent Klaiber",
            "email": "hello@vinkla.com"
        },
        {
            "name": "Jens Segers",
            "homepage": "https://jenssegers.com"
        }
    ],
    "require": {
        "php": "^5.5.9 || ^7.0",
        "composer/semver": "^1.1",
        "knplabs/packagist-api": "^1.3",
        "league/climate": "^3.2",
        "symfony/console": "^2.6 || ^3.0",
        "symfony/process": "^2.6 || ^3.0"
    },
    "require-dev": {
        "mockery/mockery": "^0.9.4",
        "phpunit/phpunit": "^4.8 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "Vinkla\\Climb\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Vinkla\\Tests\\Climb\\": "tests/"
        }
    },
    "bin": [
        "bin/climb"
    ],
    "config": {
        "preferred-install": "dist"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
